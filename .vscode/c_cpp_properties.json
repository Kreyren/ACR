{
    "configurations": [
        {
            "name": "Gitpod",
            "defines": ["ACR_PLATFORM_GITPOD"],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "clang-x64",
            "limitSymbolsToIncludedHeaders": true,
            "includePath": [
                "./include"
                ],
            "browse":
            {
                "path": [
                    "./include"
                    ]
            }
        }
    ],
    "version": 4
}
